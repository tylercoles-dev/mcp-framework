{
    "name": "@tylercoles/mcp-client-stdio",
    "version": "0.1.1",
    "description": "MCP client for stdio transport - wraps stdio MCP servers with clean TypeScript interfaces",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "npx vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "lint": "eslint src/**/*.ts",
        "clean": "rimraf dist"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "client",
        "stdio",
        "ai",
        "framework"
    ],
    "author": "Tyler Coles",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/tylercoles-dev/mcp-framework",
        "directory": "packages/mcp-client-stdio"
    },
    "homepage": "https://github.com/tylercoles-dev/mcp-framework#readme",
    "bugs": {
        "url": "https://github.com/tylercoles-dev/mcp-framework/issues"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.15.1",
        "@tylercoles/mcp-client": "^0.1.1"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "vitest": "^2.1.8",
        "@vitest/ui": "^2.1.8",
        "typescript": "^5.3.0"
    },
    "publishConfig": {
        "access": "public"
    }
}