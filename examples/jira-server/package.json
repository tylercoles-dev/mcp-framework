{
  "name": "@tylercoles/example-jira-server",
  "version": "0.1.0",
  "description": "MCP server example for Jira integration with API token authentication",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "jira-server": "node dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js",
    "start:http": "TRANSPORT=http node dist/index.js"
  },
  "dependencies": {
    "@tylercoles/mcp-server": "0.1.0",
    "@tylercoles/mcp-transport-stdio": "0.1.0",
    "@tylercoles/mcp-transport-http": "0.1.0",
    "@tylercoles/mcp-auth": "0.1.0",
    "node-fetch": "^3.3.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}